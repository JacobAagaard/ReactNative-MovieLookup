{
  "expo": {
    "name": "Movie Lookup",
    "description": "MovieLookup is a search engine for matching your favorite streaming service with IMDb.",
    "slug": "movielookup",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "githubUrl": "https://github.com/JacobAagaard/ReactNative-MovieLookup",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flonk.movielookup"
    },
    "android": {
      "package": "com.flonk.movielookup"
    }
  }
}